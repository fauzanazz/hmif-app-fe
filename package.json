{
  "name": "hmif-app-fe",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "api": "openapi --input http://127.0.0.1:5000/doc --output src/api/generated --indent 2 --useUnionTypes --useOptions --name ApiClient",
    "api-staging": "openapi --input https://api-staging.hmif.cloud/doc --output src/api/generated --indent 2 --useUnionTypes --useOptions --name ApiClient",
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx",
    "preview": "vite preview",
    "check": "pnpm run typecheck && pnpm run lint",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.28.9",
    "@tanstack/react-query-devtools": "^5.28.9",
    "@tanstack/react-router": "^1.22.9",
    "@tanstack/router-devtools": "^1.22.9",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@tanstack/router-vite-plugin": "^1.23.0",
    "@types/node": "^20.11.30",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "openapi-typescript-codegen": "^0.28.0",
    "postcss": "^8.4.38",
    "prettier": "^3.2.5",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.2.2",
    "vite": "^5.2.0"
  }
}
